//---------------------------- zad8+1

#include <stdio.h>

void bubblesort(int *tab, int dlugosc){
int i,j, temp;

for(i=0;i<dlugosc-1;i++){
for(j=0; j<dlugosc-1;j++){
if(tab[j] > tab[j+1]){
temp=tab[j];
tab[j]=tab[j+1];
tab[j+1]=temp;
}
}
}
}

int main(){
int tab[10] = {4,5,20,7,3,15,2,0,8,50};
int i;

for i=0; i<10; i++)
{ printf("%d", tab[i]); }

printf("\npo sortowaniu: \n");
bubblesort(tab,10);

for (i=0; i<10; i++){
printf(%d, tab[i]);
}
printf("\n");

return 0;
}

