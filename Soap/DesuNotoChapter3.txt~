return 0;
} 

//---------------------------- zad6+1

int fib(int a){if (a==0){return 0;} else if (a==1){return 1;} else return fib(a-1)+fib(a-2);}

#include <stdio.h>
int main(){
	int a, b;
scanf("%d %d", &a, &b)
printf("%d\n", fib(a))
return 0;
} 

