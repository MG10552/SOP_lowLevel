	printf("Podaj rozmiar tablicy: ");
	scanf("%d", &n);
int tab[n];
	printf("Podaj znak: \n");
		for (i=0;;i<n;]i++){
	scanf("%d", &tab[i]);
}
for (i=n-1; i>=0; i--) {
printf(%d, tab[i]);
}
