//---------------------------- zad3

#include <stdio.h>

int main(){
int x,y, wynik;
char znak[2];
printf("Podaj pierwszą liczbe\n");
scanf("%d", &x);
printf("Podaj podaj działanie\n");
scanf("%1s", znak);
printf("Podaj drugą liczbe\n");
scanf("%d", &y);

switch(znak)
	case '+': wynik = x+y; break;
	case '-': wynik = x-y; break;
	case '*': wynik = x*y; break;
	case '/': if (y == 0){
		 printf("error\n");
		}else{
		 wynik = x/y;} break;
	default: printf("Done nothing");
}
printf("wynik %d:", wynik);

return 0;
}

