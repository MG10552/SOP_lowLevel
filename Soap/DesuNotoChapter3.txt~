return 0;
}

AddToList(Lista* List, void* Thing)
{
	if (List->Next){
	AddToList(List->Next, Thing);
	return;
}
	Lista* NewElement = malloc(sizeof(Lista));
	List->Next = NewElement;
	NewElement->Value = Thing;
}

